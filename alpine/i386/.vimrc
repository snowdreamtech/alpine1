set fileencodings=ucs-bom,utf-8,gbk,gb2312,cp936,gb18030,big5,latin-1
set encoding=utf-8
set termencoding=utf-8
set fileencoding=utf-8

syntax on
set number
set tabstop=4
set autoindent
set wrap
set linebreak
set showmatch
set hlsearch
set incsearch
set ignorecase
set nobackup
set noswapfile
set undofile
set autoread

